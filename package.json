{
  "name": "hlife",
  "version": "1.0.1",
  "description": "ilife App Write In React-Native",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/qfant/hlife.git"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build-android": "react-native bundle --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle  --assets-dest ./android/appsrc/main/res/ --dev false --platform android",
    "build-ios": "react-native bundle --entry-file index.js --bundle-output ./ios/hlife/bundle/main.jsbundle   --assets-dest ./ios/hlife/bundle  --dev false  --platform ios",
    "build-font": "node app/components/icon/genJson.js",
    "build-android-bundle": "react-native bundle --entry-file index.js --platform android --bundle-output ./dest/android/bundle/index.android.bundle  --assets-dest ./dest/android/bundle/ --dev false",
    "build-ios-native": "react-native bundle --entry-file index.js --platform ios     --bundle-output ./dest/ios/bundle/main.jsbundle                     --assets-dest ./dest/ios/bundle/  --dev false",
    "lint": "eslint app --fix",
    "format": "find app -name '*.js' | xargs prettier --write --single-quote"
  },
  "dependencies": {
    "antd-mobile-rn": "^2.3.1",
    "crypto-js": "^3.1.9-1",
    "jcore-react-native": "^1.2.12",
    "jpush-react-native": "^2.3.0",
    "leancloud-storage": "3.6.7",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-code-push": "^5.3.2",
    "react-native-device-info": "^0.24.3",
    "react-native-elements": "^0.19.1",
    "react-native-exceptions-manager": "^0.2.0",
    "react-native-image-crop-picker": "^0.21.3",
    "react-native-linear-gradient": "^2.5.2",
    "react-native-root-siblings": "^3.1.6",
    "react-native-scrollable-tab-view": "0.8.0",
    "react-native-simple-store": "^1.3.0",
    "react-native-smart-barcode": "^1.0.6",
    "react-native-splash-screen": "3.0.6",
    "react-native-storage": "^0.2.3",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^4.6.0",
    "react-native-wechat": "^1.9.9",
    "react-native-yunpeng-alipay": "^2.0.0",
    "react-navigation": "^2.18.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "devDependencies": {
    "babel-jest": "23.4.2",
    "babel-plugin-import": "^1.8.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "23.5.0",
    "react-test-renderer": "16.4.1"
  }
}
